graph TD
    classDef open fill:#50FA7B,stroke:#333,color:#000
    classDef inprogress fill:#8BE9FD,stroke:#333,color:#000
    classDef blocked fill:#FF5555,stroke:#333,color:#000
    classDef closed fill:#6272A4,stroke:#333,color:#fff

    n0["n0<br/>n0"]
    class n0 open
    n1["n1<br/>n1"]
    class n1 open
    n2["n2<br/>n2"]
    class n2 open
    n3["n3<br/>n3"]
    class n3 open
    n4["n4<br/>n4"]
    class n4 open

    n0 ==> n1
    n0 ==> n2
    n1 ==> n3
    n2 ==> n3
    n3 ==> n4
